    <!-- Welcome screen / main sections -->
    <string name="welcome">Bienvenido al <b>Instalador del Xposed</b>!\nPor favor elija que quiere hacer:</string>
    <string name="tabInstall">Framework</string>
    <string name="tabInstallDescription">Aqui puedes administrar el Xposed Framework, que es requerido por cualquier módulo para funcionar.</string>
    <string name="tabModules">Módulos</string>
    <string name="tabModulesDescription">El Framework en sí mismo no adiciona funcionalidad alguna. Necesitas instalar módulos para eso. En esta sección puedes activar los módulos una vez los has instalado.</string>
    <string name="tabDownload">Descargar</string>
    <string name="tabDownloadDescription">Ésta seccion te da acceso al repositorio de módulos, así puedes buscar y descargar el que quieras y mantenerlo siempre actualizado.</string>
    <string name="welcome_framework_update_available">Hay una actualización para el Xposed Framework disponible (%s).</string>
    <string name="welcome_update_available">Una o más actualizaciones para los módulos están disponibles.</string>
    <string name="tabSettings">Ajustes</string>
    <string name="tabSettingsDescription">Vaya a ésta sección para configurar el Xposed Installer y el Framework.</string>
    <string name="tabSupport">Soporte</string>
    <string name="tabSupportDescription">Encuentre ayuda acerca de inconvenientes relacionados al Xposed. También puedes donar para agradecer por el Xposed.</string>
    <string name="tabLogs">Registros</string>
    <string name="tabLogsDescription">Mostrar y guardar/enviar registros del Xposed Framework.</string>
    <string name="tabAbout">Acerca de...</string>
    <string name="tabAboutDescription">Aprenda acerca de las personas quienes crearon el Xposed, para obtener soporte de ésta aplicación, etc.</string>

