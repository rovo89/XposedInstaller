    <!-- Welcome screen / main sections -->
    <string name="welcome">ברוכים הבאים ל <b>התקנת Xposed</b>!\nאנא בחרו מה ברצונכם לעשות:</string>
    <string name="tabInstall">סביבה</string>
    <string name="tabInstallDescription">פה אתה יכול לנהל את סביבת Xposed, שדרושה לכל המודולים.</string>
    <string name="tabModules">מודולים</string>
    <string name="tabModulesDescription">הסביבה עצמה אינה מוסיפה שום פונקציה. יש צורך במודולים בשביל זה. בחלק זה תוכל להפעיל מודולים שהתקנת.</string>
    <string name="tabDownload">הורדה</string>
    <string name="tabDownloadDescription">חלק זה נותן לך גישה למאגר של מודולים, כדי שתוכל לחפש ולהתקין אותם, ולשמור עליהם מעודכנים.</string>
    <string name="welcome_framework_update_available">עדכון לסביבת Xposed זמין.</string>
    <string name="welcome_update_available">קיים עדכון למודול אחד או יותר.</string>
    <string name="tabSettings">הגדרות</string>
    <string name="tabSettingsDescription">עבור לחלק זה כדי להגיד את סביבת Xposed ואת אפליקציה זו.</string>
    <string name="tabLogs">לוג</string>
    <string name="tabLogsDescription">הצגה, שמירה ושליחה של הלוג של Xposed.</string>
    <string name="tabAbout">אודות</string>
    <string name="tabAboutDescription">למד על האנשים אשר יצרו את Xposed, איפה לקבל תמיכה על אפליקציה זו ועוד.</string>

