    <string name="tabSettings">Ustawienia</string>
    <string name="tabSettingsDescription">Przejdź do tej sekcji, aby skonfigurować aplikację Xposed i proces framework.</string>
    <string name="tabLogs">Logi</string>
    <string name="tabLogsDescription">Wyświetl, zapisz i wyślij logi.</string>
    <string name="tabAbout">O aplikacji</string>
    <string name="tabAboutDescription">Dowiedz się o ludziach, którzy stworzyli Xposed, od których można uzyskać wsparcie dla tej aplikacji, itp.</string>

