    <string name="tabLogsDescription">Vis og lagre/send logger av Xposed Framework.</string>
    <string name="tabAbout">Om</string>
    <string name="tabAboutDescription">Lær mer om menneskene som har utviklet Xposed, hvor du kan få støtte for denne appen etc.</string>

